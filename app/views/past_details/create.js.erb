<% if @past_detail.errors.count == 0 %>

    $('#new_past_detail').remove();
    $('#pro-past-details').show();
    $('#past_detail_listing').append('<%= j render("past_detail", :past_detail => @past_detail) %>');

<% else %>

    $('#new_past_detail').remove();
    $('#pro-past-details').after('<%= j render("form") %>');

<% end %>